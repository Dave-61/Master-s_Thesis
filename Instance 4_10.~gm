AP4         0.4593        0.2156        0.4790        0.7987        0.3141        0.3821        0.6353        0.6440        0.3929        0.3483        0.4825        0.9764        0.7188        0.7397        0.8197        0.7845        0.5855        0.1923        0.8051        0.5681        0.4602        0.1557        0.1062        0.0194        0.3034        0.9676        0.8757        0.5193        0.8740        0.5755        0.6706        0.8110        0.8329        0.0000;

variables
          OF 'objective function: total transportation cost'
          m(i1,k) 'number of trucks on link ik, from PSi to CDk'
          n(k,j) 'number of trucks on link kj, from CDk to APj'
          z(g,j,r) 'if load is sent through a milk run trip by truck r'
          u(i1,j) 'if PSi is a direct shipment node'
          e(g,j,r) 'if PSi is the starting node of a milk run trip terminatting at APj'
          f(g,j,r) 'if PSi is the stopover node of a milk run trip terminatting at APj'
          h(g,j,r) 'if PSi is the end node of a milk run trip terminatting at APj'
          x(g,g,r,j) 'if truck r goes from PSi1 to PSi2/APj in a direct shipment/milk run trip terminating at APj'
          y(i1,k,j)'if load from PSi to APj is sent through CDk';
integer variable m(i1,k);
integer variable n(k,j);
binary variable z(g,j,r);
binary variable u(i1,j);
binary variable e(g,j,r);
binary variable f(g,j,r);
binary variable h(g,j,r);
binary variable x(g,g,r,j);
binary variable y(i1,k,j);

equations
         cost
         one(i1,j)
         extra1(j,r)
         extra2(g,r,j)
         two(i1,j,r)
         three(i1,r,j)
         four(r,j)
         five(i1,j,r)
         six(i1,j,r)
         seven(i1,j,r)
         eight(i1,j,r)
         nine(i1,j,r)
         ten(i1,j,r)
         eleven(i1,j,r)
         twelve1(i1,i2,r,j)
         thirteen(r)
         fourteen1(r)
         fourteen2(r)
         fourteen3(r)
         fourteen4(r)
         fifteen(i1,k)
         sixteen(j,k);


cost.. OF=e=sum((i1,j),u(i1,j)*C(i1,j))+sum((i1,k),m(i1,k)*C1(i1,k))+sum((k,j),n(k,j)*C2(k,j))+sum((i1,g,r,j),x(i1,g,r,j)*C(i1,g));

one(i1,j).. u(i1,j)+sum(k,y(i1,k,j))+sum(r,z(i1,j,r))=e=1;

extra1(j,r).. z(j,j,r)=e=0;
extra2(g,r,j).. x(g,g,r,j)=e=0;

two(i1,j,r).. e(i1,j,r)+f(i1,j,r)+h(i1,j,r)=e=z(i1,j,r);

three(i1,r,j).. h(i1,j,r)-x(i1,j,r,j)=e=0;

four(r,j).. sum(i1,e(i1,j,r))-sum(i1,h(i1,j,r))=e=0;

five(i1,j,r).. e(i1,j,r)+sum(i2,x(i2,i1,r,j))=l=1;

six(i1,j,r).. e(i1,j,r)+x(i1,j,r,j)=l=1;

seven(i1,j,r).. f(i1,j,r)+x(i1,j,r,j)=l=1;

eight(i1,j,r).. h(i1,j,r)+sum(i2,x(i1,i2,r,j))=l=1;

nine(i1,j,r).. sum(i2,x(i1,i2,r,j))+sum(i2,x(i2,i1,r,j))+x(i1,j,r,j)=e=2*f(i1,j,r)+2*h(i1,j,r)+e(i1,j,r);

ten(i1,j,r).. f(i1,j,r)=l=sum(i2,h(i2,j,r));

eleven(i1,j,r).. f(i1,j,r)=l=sum(i2,e(i2,j,r));

twelve1(i1,i2,r,j).. x(i1,i2,r,j)+x(i2,i1,r,j)=l=1;

thirteen(r).. sum((i1,j),h(i1,j,r))=l=1;

fourteen1(r).. sum((i1,L1),x(i1,L1,r,'AP1')*S(i1,'AP1'))=l=TY;
fourteen2(r).. sum((i1,L2),x(i1,L2,r,'AP2')*S(i1,'AP2'))=l=TY;
fourteen3(r).. sum((i1,L3),x(i1,L3,r,'AP3')*S(i1,'AP3'))=l=TY;
fourteen4(r).. sum((i1,L4),x(i1,L4,r,'AP4')*S(i1,'AP4'))=l=TY;

fifteen(i1,k).. sum(j,y(i1,k,j)*S(i1,j))=l=m(i1,k)*TY;

sixteen(j,k).. sum(i1,y(i1,k,j)*S(i1,j))=l=n(k,j)*TY;

Model Davoud /all/;

option reslim=10800;
option iterlim=100000;

solve Davoud using MIP minimizing OF;

display u.l,y.l,m.l,n.l,z.l,e.l,f.l,h.l,x.l,OF.l;